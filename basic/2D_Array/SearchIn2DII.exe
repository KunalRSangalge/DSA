/*Searching in 2D matrix when row and coloumn are sorted ,Eg-
 [[1,4,7,11,15],[2,5,8,12,19],[3,6,9,16,22],[10,13,14,17,24],[18,21,23,26,30]]
*/
#include<iostream>
#include<bits/stdc++.h>

bool searchMatrix(vector<vector<int>>& matrix, int target) {
        int s=0,e=matrix[0].size()-1;
        if(target > matrix[e][e] || target < matrix[0][0])
        return 0;
        while(s<=e){
            int mid=(s+e)/2;
            if(target == matrix[mid][mid])
            return 1;
            else if(target < matrix[mid][mid])
            e=mid-1;
            else
            s=mid+1;
            
        }
        s=0,e=matrix[0].size()-1;
        int temp=0;
        while(s<=e){
            int mid=(s+e)/2;
            if(target < matrix[mid][mid] && target > matrix[mid-1][mid-1])
            {temp=mid;break;}
            else if(target > matrix[mid][mid])
            s=mid+1;
            else
            e=mid-1;
            
        }
        //temp is the col/row in which target is there
        //col
        s=0,e=temp;
        while(s<=e){
            int mid=(s+e)/2;
            if(target == matrix[mid][temp])
            return 1;
            else if(target > matrix[mid][temp])
            s=mid+1;
            else
            e=mid-1;
        }
        //row check
        s=0,e=temp;
        while(s<=e){
            int mid=(s+e)/2;
            if(target == target == matrix[temp][mid])
            return 1;
            else if(target > target == matrix[temp][mid] )
            s=mid+1;
            else
            e=mid-1;            
        }
        
        return 0;
    }

int main(){
 vector<vector<int>>matrix[5][5]=[[1,4,7,11,15],[2,5,8,12,19],[3,6,9,16,22],[10,13,14,17,24],[18,21,23,26,30]];
 int target = 11;
searchMatrix(matrix,target);
}